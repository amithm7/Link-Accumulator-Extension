{
	"manifest_version": 2,
	"name": "Save to AmMoLiAc",
	"version": "0.1.1",

	"description": "Collects and organizes links",

	"icons": {
		"128" : "icons/Grapes.svg"
	},

	"browser_action": {
		"default_icon": "icons/Grapes.svg",
		"default_popup": "popup/popup.html"
	},

	"permissions": [
		"tabs",
		"storage",
		"unlimitedStorage"
	],

	"applications": {
		"gecko": {
			"id": "ammoliac@ammoliac"
		}
	}

}
