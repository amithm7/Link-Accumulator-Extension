{
	"manifest_version": 2,
	"name": "AmMo's Link Accumulator",
	"version": "0.1",

	"description": "Collects and organizes links",

	"icons": {
		"128" : "icons/Grapes.svg"
	},

	"browser_action": {
		"default_icon": "icons/Grapes.svg",
		"default_popup": "popup/popup.html"
	},

	"permissions": [
		"tabs",
		"storage",
		"unlimitedStorage"
	]

}
