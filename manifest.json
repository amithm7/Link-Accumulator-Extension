{
	"manifest_version": 2,
	"name": "Link Accumulator",
	"version": "0.1",

	"description": "Collects and organizes links",

	"icons": {
		"128" : "icons/Grapes.svg"
	},

	"browser_action": {
		"default_icon": "icons/Grapes.svg",
		"defaut_title": "Add to Collection",
		"default_popup": "popup/add_menu.html"
	},

	"permissions": [
		"tabs",
		"storage",
		"unlimitedStorage"
	]

}
